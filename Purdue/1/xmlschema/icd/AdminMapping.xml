      <Complex>
        <ChooserEntry elementPath="name" elementLabel="Organization Name" ftElementType="Element" toolTip="The official organization name. Organizations may be sponsors, CROs, labs, etc." stickyCase="true" />
        <ChooserEntry elementPath="organizationType" elementLabel="Type" ftElementType="Element" toolTip="Organization's role in this study" enumList="OrganizationTypes" dataType="Coded" />
        <ChooserEntry elementPath="Address/address1" elementLabel="Address 1" ftElementType="Element" toolTip="Organization address line 1" stickyCase="true" />
        <ChooserEntry elementPath="Address/address2" elementLabel="Address 2" ftElementType="Element" toolTip="Organization address line 2" stickyCase="true" />
        <ChooserEntry elementPath="Address/city" elementLabel="City" ftElementType="Element" toolTip="City in which the organization ls located." stickyCase="true" />
        <ChooserEntry elementPath="Address/stateprov" elementLabel="State / Province" ftElementType="Element" toolTip="State or province of the organization" stickyCase="true" />
        <ChooserEntry elementPath="Address/postalCode" elementLabel="Postal or Zip Code" ftElementType="Element" toolTip="Organization's postal or zip code" />
        <ChooserEntry elementPath="Address/country" elementLabel="Country" ftElementType="Element" toolTip="Organization country" stickyCase="true" />
        <ChooserEntry elementPath="Address/telephone" elementLabel="Telephone" ftElementType="Element" toolTip="Organization's telephone number - any format OK" />
        <ChooserEntry elementPath="Address/fax" elementLabel="Facsimile" ftElementType="Element" toolTip="Organization's fax number - any format OK" />
