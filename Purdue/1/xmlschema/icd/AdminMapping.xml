        <ChooserEntry elementPath="Address/postalCode" elementLabel="Postal or Zip Code" ftElementType="Element" toolTip="Contact person's zip or postal code." />
        <ChooserEntry elementPath="Address/country" elementLabel="Country" ftElementType="Element" toolTip="Contact person's country." stickyCase="true" />
